/* The following symbols are automatically detected and defined by
   the autoconf-produced configure script. */

#ifndef CONFIG_H
#define CONFIG_H

/* Define to empty if the const keyword does not work.  */
#undef const

/* Define to empty if the inline keyword does not work. */
#undef inline

/* Define if you have the following headers: */
#undef HAVE_UNISTD_H
#undef HAVE_GETOPT_H

/* Define if you have the getopt function: */
#undef HAVE_GETOPT

/* Define to use MPI: */
#undef HAVE_MPI

/* Define to use complex scalars (i.e. don't require inversion symmetry): */
#undef SCALAR_COMPLEX

/* Define to use single precision: */
#undef SCALAR_SINGLE_PREC

/* Define to use complex-Hermitian dielectric tensors: */
#undef WITH_HERMITIAN_EPSILON

/* Define if you have the FFTW library: */
#undef HAVE_LIBFFTW
#undef HAVE_LIBDFFTW
#undef HAVE_LIBSFFTW

/* check if we have FFTW: */
#if defined(HAVE_LIBFFTW) || defined(HAVE_LIBDFFTW) || defined(HAVE_LIBSFFTW)
#  define HAVE_FFTW 1
#endif

/* Define if you have the HDF5 library and headers: */
#undef HAVE_HDF5

/* Define if we have the H5Pset_mpi function for collective access: */
#undef HAVE_H5PSET_MPI

/* Define if you have the strncmp() function: */
#undef HAVE_STRNCMP

/* define to disable CHECK macros */
#undef CHECK_DISABLE

/* define to enable expensive debugging checks, etcetera */
#undef DEBUG

/* define to enable expensive debugging malloc/free routines */
#undef DEBUG_MALLOC

/* define when profiling */
#undef ENABLE_PROF

/* define if calling Fortran functions does not work: */
#undef NO_FORTRAN_FUNCTIONS

#endif /* CONFIG_H */
